PARAM_FINANCIAL_URL_KEYS     

PARAM_FINANCIAL_URL_KEYS | CREATE TABLE `PARAM_FINANCIAL_URL_KEYS` (
  `COLLECTION_TABLE` varchar(255) NOT NULL,
  `CONSTITUENT_ID` varchar(255) NOT NULL,
  `URL_KEY` varchar(255) DEFAULT NULL,
  KEY `COLLECTION_TABLE` (`COLLECTION_TABLE`),
  KEY `CONSTITUENT_ID` (`CONSTITUENT_ID`),
  CONSTRAINT `PARAM_FINANCIAL_URL_KEYS_ibfk_1` FOREIGN KEY (`COLLECTION_TABLE`) REFERENCES `PARAM_FINANCIAL_COLLECTION` (`COLLECTION_TABLE`),
  CONSTRAINT `PARAM_FINANCIAL_URL_KEYS_ibfk_2` FOREIGN KEY (`CONSTITUENT_ID`) REFERENCES `MASTER_CONSTITUENTS` (`CONSTITUENT_ID`)
)

