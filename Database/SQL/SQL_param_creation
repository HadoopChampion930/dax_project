  `COLLECTION_FOR_STORAGE` varchar(45) DEFAULT NULL,
  `PROJECT_NAME_BQ` varchar(45) DEFAULT NULL,
  `TABLE_STORE_ANALYSTS_BQ` varchar(45) DEFAULT NULL,
  `CONSTITUENT_NAME` varchar(45) DEFAULT NULL
)
  
