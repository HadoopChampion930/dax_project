ANALYST_COLLECTION  

| ANALYST_COLLECTION | CREATE TABLE `ANALYST_COLLECTION` (
  `CONSTITUENT_ID` int(11) NOT NULL,
  `ANALYST_SOURCE_URL` varchar(45) DEFAULT NULL,
  `DATE_LAST_UPDATE` int(11) DEFAULT NULL,
  `BUY_COUNT` int(11) DEFAULT NULL,
  `HOLD_COUNT` int(11) DEFAULT NULL,
  `SELL_COUNT` int(11) DEFAULT NULL,
  `MEDIAN_TARGET` int(11) DEFAULT NULL,
  `HIGHEST_TARGET` int(11) DEFAULT NULL,
  `LOWEST_TARGET` int(11) DEFAULT NULL,
  `RATING` int(11) DEFAULT NULL,
  `RECOMMENDATION` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`CONSTITUENT_ID`)
)

               
CONSTITUENT_SOURCE_TABLE  

CONSTITUENT_SOURCE_TABLE | CREATE TABLE `CONSTITUENT_SOURCE_TABLE` (
  `CONSTITUENT_ID` int(11) NOT NULL,
  `CONSTITUENT_SOURCE` varchar(45) DEFAULT NULL,
  `SOURCE` varchar(45) DEFAULT NULL,
  `STATUS` varchar(45) DEFAULT NULL,
  PRIMARY KEY (`CONSTITUENT_ID`)
)

         
FINANCIAL_KEY_COLLECTION 

FINANCIAL_KEY_COLLECTION | CREATE TABLE `FINANCIAL_KEY_COLLECTION` (
  `CONSTITUENT_ID` int(11) NOT NULL,
  `DATE` datetime DEFAULT NULL,
  `FINANCIAL_SOURCE_URL` varchar(45) DEFAULT NULL,
  `PER` int(11) DEFAULT NULL,
  `EPS` int(11) DEFAULT NULL,
  `MARKET_CAP` int(11) DEFAULT NULL,
  `TOTAL_ASSET` int(11) DEFAULT NULL,
  `EBITDA` int(11) DEFAULT NULL,
  `SALES` int(11) DEFAULT NULL,
  `NET_PROFIT` int(11) DEFAULT NULL,
  `FREE_CASH` int(11) DEFAULT NULL,
  `NET_DEBIT` int(11) DEFAULT NULL,
  `CURRENT_LBT` int(11) DEFAULT NULL,
  `DIVIDEND` int(11) DEFAULT NULL,
  `DIV_YIELD` int(11) DEFAULT NULL,
  PRIMARY KEY (`CONSTITUENT_ID`)
)




   
MASTER_CONSTITUENTS

MASTER_CONSTITUENTS | CREATE TABLE `MASTER_CONSTITUENTS` (
  `CONSTITUENT_ID` varchar(225) NOT NULL,
  `CONSTITUENT_NAME` varchar(255) NOT NULL,
  `ISIN` varchar(225) NOT NULL,
  `BVDID` varchar(255) DEFAULT NULL,
  `INDUSTRY` varchar(255) NOT NULL,
  `COUNTRY` varchar(255) NOT NULL,
  `SYMBOL` varchar(255) NOT NULL,
  `ACTIVE_STATE` tinyint(1) NOT NULL,
  `LISTED_SINCE` date NOT NULL,
  `MARKET_SECTOR` varchar(255) NOT NULL,
  `MARKET_SUBSECTOR` varchar(255) DEFAULT NULL,
  `TRADING_MODEL` varchar(255) DEFAULT NULL,
  `REUTERS_INSTRUMENT_CODE` varchar(255) NOT NULL,
  `SHARE_TYPE` varchar(255) NOT NULL,
  `MINIMUM_TRADE_UNIT` float NOT NULL,
  `WEBSITE` varchar(255) DEFAULT NULL,
  `LAST_UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `LAST_UPDATE_BY` varchar(255) NOT NULL,
  PRIMARY KEY (`CONSTITUENT_ID`)
)

         
